        <div class="topPart">
            <h2><%= locals.pass ? "Delete the file?" : "Delete this file?" %> </h2>
            <div class="file">
                <% if ((locals.contentType || locals.mimetype)?.startsWith('image/') && !locals.pass) { %>
                    <img src="/files/<%= filename %>" alt="">
                <% } else { %>
                    <div class="fileIcon">
                        <p><%= filename.split('.').pop().length <= 5 ? filename.split('.').pop() : filename.substr(0, 2) + '...' %> </p>
