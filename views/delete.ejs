                <% } else { %>
                    <div class="info name"><p>File name:</p><p><%= filename %></p></div>
                <% } %> 
                <div class="info length"><p>File size:</p><p><%= locals.size || locals.text?.length || '??' %></p></div>
                <div class="info type"><p>File type:</p><p><%= locals.contentType || locals.mimetype || (locals.text ? 'text/plain' : '??') %></p></div>
                <div class="info"><p>Expires on:</p><p class="dt"><%= new Date(datetime).toDateString() %></p></div>
                <% if (locals.pass) { %>
